{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "strict": 0,
    "no-console": 0,
    "no-unused-vars": 0,
    "no-undef": 0,
    "no-debugger": 0,
    "no-case-declarations": 0,
    "no-fallthrough": 0,
    "indent": 0,
    "linebreak-style": 0,
    "quotes": 0,
    "semi": 0
  }
}
